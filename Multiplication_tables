import java.util.Scanner;

public class Multiplication_tables {

	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		System.out.print("Start number for multiplication table: ");
		int a=s.nextInt();
		System.out.print("End number for multiplication table: ");
		int b=s.nextInt();
		System.out.println("Multiplication tables for "+a+" to "+b+" : ");
		for(int i=a;i<=b;i++) {
			for(int j=1;j<=10;j++) {
				System.out.println(i+" x "+j+" = "+(i*j));
			}
		}

	}

}
